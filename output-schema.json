{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "format": {
      "$id": "/properties/format",
      "type": "integer",
      "title": "The Format Schema ",
      "default": 0,
      "examples": [
        1
      ]
    },
    "id": {
      "$id": "/properties/id",
      "type": "string",
      "title": "The Id Schema ",
      "default": "",
      "examples": [
        "example-app.desktop"
      ]
    },
    "name": {
      "$id": "/properties/name",
      "type": "object",
      "properties": {
        "default": {
          "$id": "/properties/name/properties/default",
          "type": "string",
          "title": "The Default Schema ",
          "default": "",
          "examples": [
            "this is used in the case that there is no locale info about this text"
          ]
        },
        "en": {
          "$id": "/properties/name/properties/en",
          "type": "string",
          "title": "The En Schema ",
          "default": "",
          "examples": [
            "Example App"
          ]
        },
        "es": {
          "$id": "/properties/name/properties/es",
          "type": "string",
          "title": "The Es Schema ",
          "default": "",
          "examples": [
            "Aplicación de ejemplo"
          ]
        }
      }
    },
    "icon": {
      "$id": "/properties/icon",
      "type": "string",
      "title": "The Icon Schema ",
      "default": "",
      "examples": [
        "icon url"
      ]
    },
    "abstract": {
      "$id": "/properties/abstract",
      "type": "object",
      "properties": {
        "en": {
          "$id": "/properties/abstract/properties/en",
          "type": "string",
          "title": "The En Schema ",
          "default": "",
          "examples": [
            "Example Application Abstract."
          ]
        },
        "es": {
          "$id": "/properties/abstract/properties/es",
          "type": "string",
          "title": "The Es Schema ",
          "default": "",
          "examples": [
            "Resumen de la Applicación de Ejemplo."
          ]
        }
      }
    },
    "description": {
      "$id": "/properties/description",
      "type": "object",
      "properties": {
        "en": {
          "$id": "/properties/description/properties/en",
          "type": "string",
          "title": "The En Schema ",
          "default": "",
          "examples": [
            "<p>English long description of example app</p>"
          ]
        },
        "es": {
          "$id": "/properties/description/properties/es",
          "type": "string",
          "title": "The Es Schema ",
          "default": "",
          "examples": [
            "<p>Descripción en Español de la Aplicación de Ejemplo</p>"
          ]
        }
      }
    },
    "license": {
      "$id": "/properties/license",
      "type": "object",
      "properties": {
        "id": {
          "$id": "/properties/license/properties/id",
          "type": "string",
          "title": "The Id Schema ",
          "default": "",
          "examples": [
            "gpl3"
          ]
        },
        "name": {
          "$id": "/properties/license/properties/name",
          "type": "string",
          "title": "The Name Schema ",
          "default": "",
          "examples": [
            "GNU General Public License 3"
          ]
        },
        "body": {
          "$id": "/properties/license/properties/body",
          "type": "string",
          "title": "The Body Schema ",
          "default": "",
          "examples": [
            " ... GNU General Public License 3 ... "
          ]
        }
      }
    },
    "categories": {
      "$id": "/properties/categories",
      "type": "array",
      "items": {
        "$id": "/properties/categories/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "Development"
        ]
      }
    },
    "keywords": {
      "$id": "/properties/keywords",
      "type": "array",
      "items": {
        "$id": "/properties/keywords/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "application",
          "applicación",
          "example",
          "ejemplo"
        ]
      }
    },
    "languages": {
      "$id": "/properties/languages",
      "type": "array",
      "items": {
        "$id": "/properties/languages/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "de_DE",
          "es_CU",
          "ru"
        ]
      }
    },
    "developer": {
      "$id": "/properties/developer",
      "type": "object",
      "properties": {
        "name": {
          "$id": "/properties/developer/properties/name",
          "type": "string",
          "title": "The Name Schema ",
          "default": "",
          "examples": [
            "Bob"
          ]
        },
        "avatar": {
          "$id": "/properties/developer/properties/avatar",
          "type": "string",
          "title": "The Avatar Schema ",
          "default": "",
          "examples": [
            "https://bob.blog/logo.sng"
          ]
        },
        "website": {
          "$id": "/properties/developer/properties/website",
          "type": "string",
          "title": "The Website Schema ",
          "default": "",
          "examples": [
            "https://github.com/AppImage"
          ]
        },
        "pubkey": {
          "$id": "/properties/developer/properties/pubkey",
          "type": "string",
          "title": "The Pubkey Schema ",
          "default": "",
          "examples": [
            "gpg public key"
          ]
        }
      }
    },
    "release": {
      "$id": "/properties/release",
      "type": "object",
      "properties": {
        "date": {
          "$id": "/properties/release/properties/date",
          "type": "string",
          "title": "The Date Schema ",
          "default": "",
          "examples": [
            "10/10/2017"
          ]
        },
        "version": {
          "$id": "/properties/release/properties/version",
          "type": "string",
          "title": "The Version Schema ",
          "default": "",
          "examples": [
            "0.1.0"
          ]
        },
        "channel": {
          "$id": "/properties/release/properties/channel",
          "type": "string",
          "title": "The Channel Schema ",
          "default": "",
          "examples": [
            "stable"
          ]
        },
        "changelog": {
          "$id": "/properties/release/properties/changelog",
          "type": "object",
          "properties": {
            "en": {
              "$id": "/properties/release/properties/changelog/properties/en",
              "type": "string",
              "title": "The En Schema ",
              "default": "",
              "examples": [
                "<p>The application was created.</p>"
              ]
            },
            "es": {
              "$id": "/properties/release/properties/changelog/properties/es",
              "type": "string",
              "title": "The Es Schema ",
              "default": "",
              "examples": [
                "<p>La applicación fue creada.</p>"
              ]
            }
          }
        }
      }
    },
    "file": {
      "$id": "/properties/file",
      "type": "object",
      "properties": {
        "type": {
          "$id": "/properties/file/properties/type",
          "type": "integer",
          "title": "The Type Schema ",
          "default": 0,
          "examples": [
            2
          ]
        },
        "size": {
          "$id": "/properties/file/properties/size",
          "type": "integer",
          "title": "The Size Schema ",
          "default": 0,
          "examples": [
            1882040
          ]
        },
        "architecture": {
          "$id": "/properties/file/properties/architecture",
          "type": "string",
          "title": "The Architecture Schema ",
          "default": "",
          "examples": [
            "x86-64"
          ]
        },
        "sha512checksum": {
          "$id": "/properties/file/properties/sha512checksum",
          "type": "string",
          "title": "The Sha512checksum Schema ",
          "default": "",
          "examples": [
            "a big and fussy checksum"
          ]
        },
        "url": {
          "$id": "/properties/file/properties/url",
          "type": "string",
          "title": "The Url Schema ",
          "default": "",
          "examples": [
            "https://github.com/AppImage/AppImageKit/releases/download/continuous/appimagetool-x86_64.AppImage"
          ]
        }
      }
    },
    "screenshots": {
      "$id": "/properties/screenshots",
      "type": "array",
      "items": {
        "$id": "/properties/screenshots/items",
        "type": "object",
        "properties": {
          "height": {
            "$id": "/properties/screenshots/items/properties/height",
            "type": "integer",
            "title": "The Height Schema ",
            "default": 0,
            "examples": [
              0
            ]
          },
          "width": {
            "$id": "/properties/screenshots/items/properties/width",
            "type": "integer",
            "title": "The Width Schema ",
            "default": 0,
            "examples": [
              0
            ]
          },
          "language": {
            "$id": "/properties/screenshots/items/properties/language",
            "type": "string",
            "title": "The Language Schema ",
            "default": "",
            "examples": [
              ""
            ]
          },
          "caption": {
            "$id": "/properties/screenshots/items/properties/caption",
            "type": "string",
            "title": "The Caption Schema ",
            "default": "",
            "examples": [
              "Update from Ubuntu with AppImageUpdate"
            ]
          },
          "url": {
            "$id": "/properties/screenshots/items/properties/url",
            "type": "string",
            "title": "The Url Schema ",
            "default": "",
            "examples": [
              "https://cloud.githubusercontent.com/assets/2480569/19410850/0390fe9c-92f6-11e6-9882-3ca6d360a190.jpg"
            ]
          }
        }
      }
    },
    "mime-types": {
      "$id": "/properties/mime-types",
      "type": "array",
      "items": {
        "$id": "/properties/mime-types/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "application/x-iso9660-image",
          "application/vnd.appimage"
        ]
      }
    },
    "links": {
      "$id": "/properties/links",
      "type": "object",
      "properties": {
        "homepage": {
          "$id": "/properties/links/properties/homepage",
          "type": "string",
          "title": "The Homepage Schema ",
          "default": "",
          "examples": [
            "https://appimage.org"
          ]
        },
        "help": {
          "$id": "/properties/links/properties/help",
          "type": "string",
          "title": "The Help Schema ",
          "default": "",
          "examples": [
            "https://github.com/AppImage/AppImageKit/wiki"
          ]
        },
        "donation": {
          "$id": "/properties/links/properties/donation",
          "type": "string",
          "title": "The Donation Schema ",
          "default": "",
          "examples": [
            "https://www.paypal.me/azubieta"
          ]
        },
        "development": {
          "$id": "/properties/links/properties/development",
          "type": "string",
          "title": "The Development Schema ",
          "default": "",
          "examples": [
            "https://github.com/AppImage/AppImageKit"
          ]
        }
      }
    }
  }
}
